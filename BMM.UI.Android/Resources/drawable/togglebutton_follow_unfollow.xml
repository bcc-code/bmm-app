<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_checked="true" >
    <shape>
      <solid
          android:color="#FFFFFF" />
      <stroke
          android:width="1dp"
          android:color="#03D67F" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="8dp"
          android:top="8dp"
          android:right="8dp"
          android:bottom="8dp" />
    </shape>
  </item>
  <!-- pressed -->

  <item>
    <shape>
      <solid
          android:color="#FFFFFF" />
      <stroke
          android:width="1dp"
          android:color="#0D131A" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="8dp"
          android:top="8dp"
          android:right="8dp"
          android:bottom="8dp" />
    </shape>
  </item>
  <!-- default/unchecked -->
</selector>