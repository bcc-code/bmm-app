<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/LeftItemLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_xmedium"
        android:layout_marginTop="@dimen/margin_xmedium"
        android:layout_marginBottom="@dimen/margin_xxsmall"
        local:layout_constraintStart_toStartOf="parent"
        local:layout_constraintTop_toTopOf="parent"
        local:layout_constraintBottom_toBottomOf="parent"
        local:MvxBind="Text LeftItemLabel"
        style="@style/Subtitle2_Label3" />

    <TextView
        android:id="@+id/RightItemLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_xmedium"
        android:layout_marginTop="@dimen/margin_xmedium"
        android:layout_marginBottom="@dimen/margin_xxsmall"
        local:layout_constraintEnd_toEndOf="parent"
        local:layout_constraintTop_toTopOf="parent"
        local:layout_constraintBottom_toBottomOf="parent"
        local:MvxBind="Text RightItemLabel"
        style="@style/Subtitle2_Label3" />

</androidx.constraintlayout.widget.ConstraintLayout>